<% if @resource.errors.present? %>
  <% @resource.errors.each do |key,error| %>
    console.log("<%= error.to_s %>");
  <% end %>
<% else %>
  <% if false %>
    // not doing this for now
    $('#<%=j dom_id(@resource.attachable) %>').html('<%=j render "blocks/main_blocks_partial", block: @resource.attachable %>')
  <% end %>
<% end %>
